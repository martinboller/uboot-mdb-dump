
config system
	option hostname 'Router'
	option timezone 'GMT-8'
	option zonename 'GMT-8'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '1'
	list server 'ntp.aliyun.com'
	list server 'ntp1.aliyun.com'
	list server 'ntp2.aliyun.com'

config led
	option default '0'
	option name '2_4gwifi'
	option sysfs 'ra0'
	option trigger 'netdev'
	option dev 'ra0'
	option mode 'link tx rx'

config led
	option default '0'
	option name 'usb'
	option sysfs 'usb'
	option trigger 'usbport'
	list port 'usb1-port1'
