config serial 'main'
        option unixsock "/tmp/any_mqttd"
        option unixudpsock "/tmp/any_mqttd_udp"
	option script "/usr/sbin/serial_script"
	option host "epplink.net"
        option port "1883" 
        option username "" 
        option password "" 
        option keepalive "60"
        option qos "0"
        option subqos "0"
        option clean_session "1"
	option id ""
	option topic ""
	option subtopic ""
        option insecure "1"
        option debug "4"
